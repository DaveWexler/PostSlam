		<div class="row">
			<div class="col-xs-12 col-sm-6 col-sm-offset-3 col-md-4 col-md-offset-4">
				<form class="loginRegistrationForm">
					<h1 class="text-center">Login</h1>
					<div class="input">
						<input type="email" class="form-control input-field">
						<label class="input-label">
							<span class="input-label-content">Email Address</span>
						</label>
					</div>
					<div class="input">
						<input type="password" class="form-control input-field">
						<label class="input-label">
							<span class="input-label-content">Password</span>
						</label>
					</div>
					<a href="#" title="#" class="forgotPassword">Forgot your password?</a>
					<button type="submit" class="btn btn-lg btn-yellow btn-effect">Login</button>
				</form>
			</div>
		</div>
	</div>

